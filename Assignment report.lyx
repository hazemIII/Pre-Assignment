\begin_layout Plain Layout

	def __init__(self , root):         
\end_layout

\begin_layout Plain Layout

		self.left = None         
\end_layout

\begin_layout Plain Layout

		self.right = None         
\end_layout

\begin_layout Plain Layout

		self.root = root     
\end_layout

\begin_layout Plain Layout

	def left(self):         
\end_layout

\begin_layout Plain Layout

		return self.left     
\end_layout

\begin_layout Plain Layout

	def right(self):         
\end_layout

\begin_layout Plain Layout

		return self.right     
\end_layout

\begin_layout Plain Layout

	def setval(self,val):         
\end_layout

\begin_layout Plain Layout

		self.root = val     
\end_layout

\begin_layout Plain Layout

	def getval(self):         
\end_layout

\begin_layout Plain Layout

		return self.root     
\end_layout

\begin_layout Plain Layout

	def insert(self,item):         
\end_layout

\begin_layout Plain Layout

		if (item < self.getval()):             
\end_layout

\begin_layout Plain Layout

			if (self.left != None):                 
\end_layout

\begin_layout Plain Layout

				self.left.insert(item)             
\end_layout

\begin_layout Plain Layout

			else:                 
\end_layout

\begin_layout Plain Layout

				self.left = BST(item)         
\end_layout

\begin_layout Plain Layout

			else:             
\end_layout

\begin_layout Plain Layout

				if (self.right != None):                 
\end_layout

\begin_layout Plain Layout

					self.right.insert(item)             
\end_layout

\begin_layout Plain Layout

				else:                 
\end_layout

\begin_layout Plain Layout

					self.right = BST(item)
\end_layout

\begin_layout Plain Layout

	def inorder (tree):         
\end_layout

\begin_layout Plain Layout

		if tree != None:             
\end_layout

\begin_layout Plain Layout

			if tree.left !=None:                 
\end_layout

\begin_layout Plain Layout

				BST.inorder(tree.left)             
\end_layout

\begin_layout Plain Layout

			print(tree.getval())             
\end_layout

\begin_layout Plain Layout

			if tree.right !=None:                 
\end_layout

\begin_layout Plain Layout

				BST.inorder(tree.right)
\end_layout

\begin_layout Plain Layout

	def stepCounter (tree , counter=0):         
\end_layout

\begin_layout Plain Layout

		if tree != None:             
\end_layout

\begin_layout Plain Layout

			if tree.left !=None:                 
\end_layout

\begin_layout Plain Layout

				counter=BST.stepCounter(tree.left , counter+1)
\end_layout

\begin_layout Plain Layout

            if tree.right !=None:                 
\end_layout

\begin_layout Plain Layout

				counter=BST.stepCounter(tree.right , counter+1)         
\end_layout

\begin_layout Plain Layout

		return counter
